{
  "name": "convert-to-imgur",
  "version": "1.0.0",
  "description": "تحويل صور قوالب mybb إلى مركز رفع الصور imgur",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package:linux32": "electron-packager . --platform=linux --arch=ia32 --version-string.CompanyName=\"xlmnxp\" --out=../builds --overwrite",
    "package:linux64": "electron-packager . --platform=linux --arch=x64 --version-string.CompanyName=\"xlmnxp\" --out=../builds --overwrite",
    "package:win32": "electron-packager . --platform=win32 --arch=ia32 --version-string.CompanyName=\"xlmnxp\" --out=../builds --overwrite",
    "package:win64": "electron-packager . --platform=win32 --arch=x64 --version-string.CompanyName=\"xlmnxp\" --out=../builds --overwrite",
    "package:mac": "electron-packager . --platform=darwin --arch=x64 --version-string.CompanyName=\"xlmnxp\" --out=../builds --overwrite"
  },
  "keywords": [
    "template","to","imgur"
  ],
  "author": "xlmnxp",
  "homepage": "http://www.runbb.net/",
  "devDependencies": {
    "electron-prebuilt": "^1.2.7"
  },
  "dependencies": {
    "electron": "^1.8.4",
    "imgur": "^0.2.1"
  }
}
